{{template "base" .}}

{{define "content"}}
<div class="container">
  <div class="row">
    <div class="col">
      <h1>Chose a room</h1>

      {{$rooms := index .Data "rooms"}}
      <ul>
        {{range $rooms}}
        <li>
          <a href="/chose-room/{{.ID}}">{{.RoomName}}</a>
        </li>
        {{
          end
        }}
      </ul>
    </div>
  </div>
</div>
{{ end }}
